<panel
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='../WindowXmlSchema.xsd'>
    <id>1</id>
    <type>Panel</type>
    <active>true</active>
    <background>/image/5.jpg</background>
    <component>
        <id>2</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>860</x>
                <y>70</y>
                <width>600</width>
                <heigh>80</heigh>
            </rect>
            <text>移动商务银行服务系统</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>40</fontSize>
            </setFontAction>
        </action>
    </component>
	
    <component>
        <id>3</id>
        <type>Button</type>
        <attribute>
            <text>个人业务</text>
            <rect>
                <x>900</x>
                <y>180</y>
                <width>300</width>
                <heigh>100</heigh>
            </rect>
            <debug>false</debug>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>30</fontSize>
            </setFontAction>
            <jumpPanelAction>
                <nextPanel>10</nextPanel>
            </jumpPanelAction>
        </action>
    </component>
	
    <component>
        <id>4</id>
        <type>Button</type>
        <attribute>
            <text>企业业务</text>
            <rect>
                <x>900</x>
                <y>330</y>
                <width>300</width>
                <heigh>100</heigh>
            </rect>
            <debug>false</debug>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>30</fontSize>
            </setFontAction>
            <jumpPanelAction>
                <nextPanel>20</nextPanel>
            </jumpPanelAction>
        </action>
    </component>
	
