    <component>
        <id>6</id>
        <type>Button</type>
        <attribute>
            <text>社保查询</text>
            <rect>
                <x>900</x>
                <y>480</y>
                <width>300</width>
                <heigh>100</heigh>
            </rect>
            <debug>false</debug>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>30</fontSize>
            </setFontAction>
            <jumpPanelAction>
                <nextPanel>50</nextPanel>
            </jumpPanelAction>
        </action>
    </component>
	
	
    <component>
        <id>7</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>340</x>
                <y>930</y>
                <width>1024</width>
                <heigh>150</heigh>
            </rect>
            <text>欢迎使用移动商务银行服务系统,请按相应功能键获取服务</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>25</fontSize>
            </setFontAction>
        </action>
    </component>
    
  <component>
        <id>8</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>10</x>
                <y>20</y>
                <width>200</width>
                <heigh>40</heigh>
            </rect>
            <text>终端编号：${TERMINAL_ID}</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>15</fontSize>
            </setFontAction>
        </action>
    </component>
    
     <component>
        <id>9</id>
        <type>VideoPlayer</type>
        <attribute>
            <rect>
                <x>10</x>
                <y>200</y>
                <width>850</width>
                <heigh>622</heigh>
            </rect>
        </attribute>
        <action>
            <playVideoAction>
                <media>./media/video/anhui.mp4</media>
            </playVideoAction>
        </action>
    </component>
    
     <component>
        <id>100001</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>220</x>
                <y>20</y>
                <width>300</width>
                <heigh>40</heigh>
            </rect>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>15</fontSize>
            </setFontAction>
            <showClockAction>
                <!-- you can learn how to define date-time format -->
                <!-- https://docs.oracle.com/javase/8/docs/api/java/util/Formatter.html#Formatter-java.util.Locale- 
                default is <format>%1$tc</format> -->
                <!--format delete %1$tT-->
                <format></format>
            </showClockAction>
        </action>
    </component>
    

    
