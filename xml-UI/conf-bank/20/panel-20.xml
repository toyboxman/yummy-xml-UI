<panel
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='../WindowXmlSchema.xsd'>
    <id>20</id>
    <type>Panel</type>
    <active>false</active>
	<background>/image/3.jpg</background>
    <component>
        <id>21</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>500</x>
                <y>130</y>
                <width>600</width>
                <heigh>80</heigh>
            </rect>
            <text>企业业务</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>40</fontSize>
            </setFontAction>
        </action>
    </component>
	
    <component>
        <id>22</id>
        <type>Button</type>
        <attribute>
            <text>账户管理</text>
            <rect>
                <x>200</x>
                <y>240</y>
                <width>300</width>
                <heigh>100</heigh>
            </rect>
            <debug>
        		<color>255,0,0</color>
            </debug>
            
        </attribute>
        <action>
			<setFontAction>
                <fontSize>30</fontSize>
            </setFontAction>
            <jumpPanelAction>
                <nextPanel>30</nextPanel>
            </jumpPanelAction>
        </action>
    </component>
	
	<component>
        <id>23</id>
        <type>Button</type>
        <attribute>
            <text>资金划转</text>
            <rect>
                <x>800</x>
                <y>240</y>
                <width>300</width>
                <heigh>100</heigh>
            </rect>
            <debug>
        		<color>255,0,0</color>
            </debug>
            
        </attribute>
        <action>
		 <setFontAction>
                <fontSize>30</fontSize>
            </setFontAction>
            <jumpPanelAction>
                <nextPanel>40</nextPanel>
            </jumpPanelAction>
        </action>
    </component>
	
	<component>
        <id>26</id>
        <type>Button</type>
        <attribute>
            <text>返回</text>
            <rect>
                <x>800</x>
                <y>800</y>
                <width>250</width>
                <heigh>80</heigh>
            </rect>
        </attribute>
        <action>
            <jumpPanelAction>
                <nextPanel>1</nextPanel>
            </jumpPanelAction>
        </action>
    </component>
	
	<component>
        <id>27</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>340</x>
                <y>930</y>
                <width>1024</width>
                <heigh>150</heigh>
            </rect>
            <text>欢迎使用移动商务银行服务系统,请按相应功能键获取服务</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>25</fontSize>
            </setFontAction>
        </action>
    </component>
</panel>
