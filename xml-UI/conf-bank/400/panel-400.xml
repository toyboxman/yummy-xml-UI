    <component>
        <id>40006</id>
        <type>Button</type>
        <attribute>
            <text>返回</text>
            <rect>
                <x>800</x>
                <y>800</y>
                <width>250</width>
                <heigh>80</heigh>
            </rect>
        </attribute>
        <action>
            <buttonJumpPanelAction>
                <nextPanel>10</nextPanel>
            </buttonJumpPanelAction>
        </action>
    </component>
</panel>
