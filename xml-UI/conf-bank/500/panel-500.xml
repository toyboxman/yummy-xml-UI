    xsi:noNamespaceSchemaLocation='../WindowXmlSchema.xsd'>
    <id>500</id>
    <type>Panel</type>
    <active>false</active>
	<background>/image/4.jpg</background>
    <component>
        <id>50001</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>500</x>
                <y>70</y>
                <width>600</width>
                <heigh>150</heigh>
            </rect>
            <text>指纹银行</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>45</fontSize>
            </setFontAction>
        </action>
    </component>
	
    <component>
        <id>50002</id>
        <type>Button</type>
        <attribute>
            <text>指纹绑定</text>
            <rect>
                <x>200</x>
                <y>240</y>
                <width>300</width>
                <heigh>80</heigh>
            </rect>
            <debug>false</debug>
        </attribute>
        <action>
            <jumpPanelAction>
                <nextPanel>5000201</nextPanel>
            </jumpPanelAction>
        </action>
    </component>
	
	<component>
        <id>50003</id>
        <type>Button</type>
