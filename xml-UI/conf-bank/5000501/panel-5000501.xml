<panel
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='../WindowXmlSchema.xsd'>
    <id>5000501</id>
    <type>Panel</type>
    <active>false</active>
	<background>/image/3.jpg</background>
	<component>
        <id>5000502</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>450</x>
                <y>70</y>
                <width>600</width>
                <heigh>150</heigh>
            </rect>
            <text>指纹跨行转账</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>45</fontSize>
            </setFontAction>
        </action>
    </component>
	
	<component>
        <id>5000503</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>300</x>
                <y>230</y>
                <width>200</width>
                <heigh>60</heigh>
            </rect>
            <text>付款卡号:</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>35</fontSize>
            </setFontAction>
        </action>
    </component>

	<component>
        <id>5000504</id>
        <type>ComboBox</type>
        <attribute>
            <rect>
                <x>500</x>
                <y>230</y>
                <width>430</width>
                <heigh>60</heigh>
            </rect>
        </attribute>
		
		<action>
            <showComboBoxAction>
                <items>/, ACTION4/读银行卡</items>
            </showComboBoxAction>
            <swipe2In1CardAction>
                <nextCursor>3000310</nextCursor>
                <editable>true</editable>
            </swipe2In1CardAction>
        </action>
		
		<action>
			<playMediaAction>
                <media>./media/accoutfrom.wav</media>
            </playMediaAction>
        </action>
    </component>
	
	<component>
        <id>5000505</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>300</x>
                <y>330</y>
                <width>200</width>
                <heigh>60</heigh>
            </rect>
            <text>转账金额:</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>35</fontSize>
            </setFontAction>
        </action>
    </component>
	
	<component>
        <id>5000506</id>
        <type>TextField</type>
        <attribute>
            <rect>
                <x>500</x>
                <y>330</y>
                <width>200</width>
                <heigh>60</heigh>
            </rect>
        </attribute>
		
		<action>
			<playMediaAction>
                <media>./media/money.wav</media>
            </playMediaAction>
			
			<limitInputAction>
                <length>16</length>
                <enableCashLimit>true</enableCashLimit>
            </limitInputAction>
        </action>
		
    </component>
	
	<component>
        <id>5000507</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>300</x>
                <y>430</y>
                <width>200</width>
                <heigh>60</heigh>
            </rect>
            <text>收款帐号:</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>35</fontSize>
            </setFontAction>
        </action>
    </component>

	<component>
        <id>5000508</id>
        <type>ComboBox</type>
        <attribute>
            <rect>
                <x>500</x>
                <y>430</y>
                <width>430</width>
                <heigh>60</heigh>
            </rect>
        </attribute>
		
		<action>
            <showComboBoxAction>
                <items>/, ACTION4/读银行卡</items>
            </showComboBoxAction>
            <swipe2In1CardAction>
                <nextCursor>3000308</nextCursor>
                <editable>true</editable>
            </swipe2In1CardAction>
        </action>
		
		<action>
			<playMediaAction>
                <media>./media/accoutto.wav</media>
            </playMediaAction>
        </action>
    </component>
	

	<component>
        <id>5000509</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>300</x>
                <y>530</y>
                <width>200</width>
                <heigh>60</heigh>
            </rect>
            <text>收款户名:</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>35</fontSize>
            </setFontAction>
        </action>
    </component>

	<component>
        <id>5000510</id>
        <type>TextField</type>
        <attribute>
            <rect>
                <x>500</x>
                <y>530</y>
                <width>300</width>
                <heigh>60</heigh>
            </rect>
        </attribute>
		
		<action>
			<playMediaAction>
                <media>./media/name.wav</media>
            </playMediaAction>
        </action>
		
		<action>
			<virtualKeyboardAction>
                <start>AVF_Display.exe</start>
                <stop>AVF_Hide.exe</stop>
            </virtualKeyboardAction>
        </action>
    </component>
	
	
	
	
	<component>
        <id>5000511</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>290</x>
                <y>630</y>
                <width>100</width>
                <heigh>60</heigh>
            </rect>
            <text>指纹密码:</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>35</fontSize>
            </setFontAction>
        </action>
    </component>
	
	<component>
        <id>5000512</id>
        <type>PasswordField</type>
        <attribute>
            <rect>
                <x>500</x>
                <y>630</y>
                <width>150</width>
                <heigh>60</heigh>
            </rect>
        </attribute>
			
		<action>
            <limitInputAction>
                <length>6</length>
            </limitInputAction>
			
			<playMediaAction>
                <media>./media/passwd.wav</media>
            </playMediaAction>
        </action>
    </component>
	
	
    <component>
        <id>5000513</id>
        <type>Button</type>
		
        <attribute>
            <rect>
                <x>500</x>
                <y>850</y>
                <width>250</width>
                <heigh>80</heigh>
            </rect>
            <text>提交</text>
        </attribute>
		
        <action>
            <customizedAction>
                <className></className>
            </customizedAction>
            <setFontAction>
                <fontSize>35</fontSize>
            </setFontAction>
        </action>
		
		<action>
            <sendMsgAction>
                <prsCode>zwdiffTranQuery</prsCode>
                <conditions>5000404,5000406,5000408,5000410,5000412</conditions>
				
                <nextStep>  
                    <nextPanel>30008901</nextPanel> 
                    <display>30008903</display>  
                </nextStep>
				
                <exception> 
                    <nextPanel>99901</nextPanel>  
                    <display>99904</display>  
                </exception>
				
				
				<checkRules>
					<notNull>
                        <content>5000404</content>
                        <errMsg>付款卡号不能为空</errMsg>
                    </notNull>
					
                    <notNull>
                        <content>5000408</content>
                        <errMsg>收款账号不能为空</errMsg>
                    </notNull>
					
					<notNull>
                        <content>5000410</content>
                        <errMsg>收款户名不能为空</errMsg>
                    </notNull>
					
					<validateCJK>
                        <content>5000510</content>
                        <errMsg>收款户名必须是中文</errMsg>
                    </validateCJK>
					
					<notNull>
                        <content>5000406</content>
                        <errMsg>转账金额不能为空</errMsg>
                    </notNull>
					
					<notNull>
                        <content>5000412</content>
                        <errMsg>密码不能为空</errMsg>
                    </notNull>
					
	
                    <template>
                        <content>5000408</content>
                        <pattern>6214</pattern>
                        <errMsg>收款账户非本行卡片，请使用本行卡</errMsg>
                    </template>
				</checkRules>	
				
				
            </sendMsgAction>
        </action>
    </component>
	
    <component>
        <id>5000514</id>
        <type>Button</type>
        <attribute>
            <rect>
                <x>800</x>
                <y>850</y>
                <width>250</width>
                <heigh>80</heigh>
            </rect>
            <text>返回</text>
        </attribute>
        <action>
            <jumpPanelAction>
                <nextPanel>500</nextPanel>
            </jumpPanelAction>
			<setFontAction>
                <fontSize>35</fontSize>
            </setFontAction>
        </action>
		<action>
            <cleanAction>
                <conditions>5000404,5000406,5000408,5000410,5000412</conditions>
            </cleanAction>
        </action>   
    </component>
	
	
</panel>
