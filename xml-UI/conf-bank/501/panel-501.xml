<panel
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='../WindowXmlSchema.xsd'>
    <id>5011</id>
    <type>Panel</type>
    <active>false</active>
	<background>/image/2.jpg</background>
	<component>
        <id>5012</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>430</x>
                <y>70</y>
                <width>600</width>
                <heigh>150</heigh>
            </rect>
            <text>申报信息状态</text>
        </attribute>
        <action>
            <fontSetAction>
                <fontSize>45</fontSize>
            </fontSetAction>
        </action>
    </component>
	
	
	<component>
        <id>5013</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>300</x>
                <y>230</y>
                <width>300</width>
                <heigh>60</heigh>
            </rect>
            <text>身份证:</text>
        </attribute>
        <action>
            <fontSetAction>
                <fontSize>35</fontSize>
            </fontSetAction>
        </action>
    </component>
	
	
	
	
	<component>
        <id>5014</id>
        <type>TextField</type>
        <attribute>
            <rect>
                <x>450</x>
                <y>230</y>
                <width>400</width>
                <heigh>60</heigh>
            </rect>
        </attribute>
		<action>
			
			
			<customizedAction>
                <className>king.flow.action.customization.CusorMoveAction</className>
                <constructorParameters>
                    <parameter>
                        <type>int</type>
                        <value>5014</value>
                    </parameter>
                    <parameter>
                        <type>int</type>
                        <value>5016</value>
                    </parameter>
                </constructorParameters>
			</customizedAction>
        </action>
    </component>
	
	
	<component>
        <id>5015</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>300</x>
                <y>330</y>
                <width>300</width>
                <heigh>60</heigh>
            </rect>
            <text>密  码:</text>
        </attribute>
        <action>
            <fontSetAction>
                <fontSize>35</fontSize>
            </fontSetAction>
        </action>

    </component>
	
	<component>
        <id>5016</id>
        <type>PasswordField</type>
        <attribute>
            <rect>
                <x>450</x>
                <y>330</y>
                <width>150</width>
                <heigh>60</heigh>
            </rect>
        </attribute>
		
		<action>
            <limitInputAction>
                <length>6</length>
            </limitInputAction>
			
			<mediaPlayAction>
                <media>./media/passwd.wav</media>
            </mediaPlayAction>
			
			
			<customizedAction>
                <className>king.flow.action.customization.CusorMoveAction</className>
                <constructorParameters>
                    <parameter>
                        <type>int</type>
                        <value>5014</value>
                    </parameter>
                    <parameter>
                        <type>int</type>
                        <value>5018</value>
                    </parameter>
                </constructorParameters>
			</customizedAction>
			
			
			
        </action>
    </component>
	
	
	
    <component>
        <id>5017</id>
        <type>Button</type>
        <attribute>
            <rect>
                <x>430</x>
                <y>430</y>
                <width>250</width>
                <heigh>80</heigh>
            </rect>
            <text>提交</text>
        </attribute>
        <action>
            <msgSendAction>
                <prsCode>querysbsbzt</prsCode>
                <conditions>5014, 5016</conditions>
                <nextStep>  
                    <nextPanel>5011</nextPanel> 
                    <display>50110</display>  
                </nextStep>
                <exception> 
                    <nextPanel>999</nextPanel>  
                    <display>9993</display>  
                </exception>
				
				
				<checkRules>
					<notNull>
                        <content>5014</content>
                        <errMsg>身份证号不能为空</errMsg>
                    </notNull>
					
                    <notNull>
                        <content>5016</content>
                        <errMsg>密码不能为空</errMsg>
                    </notNull>
                </checkRules>

				
            </msgSendAction>
			
			
			
			<customizedAction>
                <className>king.flow.action.customization.CusorMoveAction</className>
                <constructorParameters>
                    <parameter>
                        <type>int</type>
                        <value>5014</value>
                    </parameter>
                    <parameter>
                        <type>int</type>
                        <value>5018</value>
                    </parameter>
                </constructorParameters>
			</customizedAction>
        </action>
    </component>
	
    <component>
        <id>5018</id>
        <type>Button</type>
        <attribute>
            <rect>
                <x>700</x>
                <y>430</y>
                <width>250</width>
                <heigh>80</heigh>
            </rect>
            <text>返回</text>
        </attribute>
        <action>
            <buttonJumpPanelAction>
                <nextPanel>50</nextPanel>
            </buttonJumpPanelAction>
        </action>
		<action>
            <cleanAction>
                <conditions>5014,5016</conditions>
            </cleanAction>
        </action> 

		
    </component>
	
	
    <decorator>
        <id>5019</id>
        <type>ScrollPanel</type>
        <attribute>
            <rect>
                <x>0</x>
                <y>560</y>
                <width>1280</width>
                <heigh>400</heigh>
            </rect>
            <debug>false</debug>
        </attribute>
        <component>
            <id>50110</id>
            <type>Table</type>
            <attribute>
                <rect>
                    <x>0</x>
                    <y>560</y>
                    <width>1280</width>
                    <heigh>400</heigh>
                </rect>
                <debug>false</debug>
            </attribute>
            <action>
                <tableShowAction>
                    <columnNames>申报流水号, 申报时间, 申报业务类型, 四险反馈状态, 医疗反馈状态</columnNames>
                </tableShowAction>
				<fontSetAction>
                <fontSize>22</fontSize>
				</fontSetAction>
            </action>
        </component>
    </decorator>
	
	
	<component>
        <id>50111</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>0</x>
                <y>970</y>
                <width>1024</width>
                <heigh>70</heigh>
            </rect>
            <text>   申报信息状态</text>
			<debug>false</debug>
        </attribute>
        <action>
            <fontSetAction>
                <fontSize>25</fontSize>
            </fontSetAction>
        </action>
    </component>
</panel>
