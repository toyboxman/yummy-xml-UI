        </attribute>
        <action>
            <setFontAction>
                <fontSize>35</fontSize>
            </setFontAction>
        </action>
    </component>
	
	
	
	
	<component>
        <id>5024</id>
        <type>TextField</type>
        <attribute>
            <rect>
                <x>450</x>
                <y>230</y>
                <width>400</width>
                <heigh>60</heigh>
            </rect>
        </attribute>
		<action>
			
			
			<customizedAction>
                <className>king.flow.action.customization.CusorMoveAction</className>
                <constructorParameters>
                    <parameter>
                        <type>int</type>
                        <value>5014</value>
                    </parameter>
                    <parameter>
                        <type>int</type>
                        <value>5016</value>
                    </parameter>
                </constructorParameters>
			</customizedAction>
        </action>
    </component>
	
	
	<component>
        <id>5025</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>300</x>
                <y>330</y>
                <width>300</width>
                <heigh>60</heigh>
            </rect>
            <text>密  码:</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>35</fontSize>
            </setFontAction>
        </action>

    </component>
	
	<component>
        <id>5026</id>
        <type>PasswordField</type>
        <attribute>
            <rect>
                <x>450</x>
                <y>330</y>
                <width>150</width>
                <heigh>60</heigh>
            </rect>
        </attribute>
		
		<action>
            <limitInputAction>
                <length>6</length>
            </limitInputAction>
			
			<playMediaAction>
                <media>./media/passwd.wav</media>
            </playMediaAction>
			
			
			<customizedAction>
                <className>king.flow.action.customization.CusorMoveAction</className>
                <constructorParameters>
                    <parameter>
                        <type>int</type>
                        <value>5024</value>
                    </parameter>
                    <parameter>
                        <type>int</type>
                        <value>5028</value>
                    </parameter>
                </constructorParameters>
			</customizedAction>
			
			
			
        </action>
    </component>
	
	
    <component>
        <id>5027</id>
        <type>Button</type>
        <attribute>
            <rect>
                <x>400</x>
                <y>650</y>
                <width>250</width>
                <heigh>80</heigh>
            </rect>
            <text>提交</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>30</fontSize>
            </setFontAction>
        </action>
        <action>
            <sendMsgAction>
                <prsCode>querysbkblzt</prsCode>
                <conditions>5024,5026</conditions>
                <nextStep>  
                    <nextPanel>500881</nextPanel> 
                    <display>500883</display>  
                </nextStep>
                <exception> 
                    <nextPanel>999</nextPanel>  
                    <display>9993</display>  
                </exception>
				
				<checkRules>
					<notNull>
                        <content>5024</content>
                        <errMsg>身份证号不能为空</errMsg>
                    </notNull>
					
                    <notNull>
                        <content>5026</content>
                        <errMsg>密码不能为空</errMsg>
                    </notNull> 
                </checkRules>
            </sendMsgAction>
			
			<customizedAction>
                <className>king.flow.action.customization.CusorMoveAction</className>
                <constructorParameters>
                    <parameter>
                        <type>int</type>
                        <value>5026</value>
                    </parameter>
                    <parameter>
                        <type>int</type>
                        <value>5028</value>
                    </parameter>
                </constructorParameters>
			</customizedAction>
        </action>
    </component>
	
    <component>
        <id>5028</id>
        <type>Button</type>
        <attribute>
            <rect>
                <x>750</x>
                <y>650</y>
                <width>250</width>
                <heigh>80</heigh>
