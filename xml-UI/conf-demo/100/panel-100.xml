<panel
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='../../conf/WindowXmlSchema.xsd'>
    <id>100</id>
    <type>Panel</type>
    <active>true</active>
    <component>
        <id>101</id>
        <type>Button</type>
        <attribute>
            <text>个人版</text>
            <rect>
                <x>200</x>
                <y>300</y>
                <width>300</width>
                <heigh>50</heigh>
            </rect>
            <debug>false</debug>
        </attribute>
        <action>
            <webLoadAction>
                <url>http://www.huanqiu.com/</url>
            </webLoadAction>
        </action>
    </component>
    <component>
        <id>102</id>
        <type>Button</type>
        <attribute>
            <text>企业版</text>
            <rect>
                <x>700</x>
                <y>300</y>
                <width>300</width>
                <heigh>50</heigh>
            </rect>
        </attribute>
        <action>
            <webLoadAction>
                <url>http://www.baidu.com/</url>
            </webLoadAction>
        </action>
    </component>
</panel>
