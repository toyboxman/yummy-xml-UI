<?xml version="1.0" encoding="UTF-8"?>
<panel
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='../../schema/WindowXmlSchema.xsd'>
    <id>30604</id>
    <type>Panel</type>
    <active>false</active>
    <background>/image/2.png</background>
    <component>	
        <id>3060402</id>
        <type>Label</type>
        <attribute>
            <rect>
                <x>500</x>
                <y>200</y>
                <width>500</width>
                <heigh>40</heigh>
            </rect>
            <text>门诊清单查询</text>
        </attribute>
        <action>
            <setFontAction>
                <fontSize>40</fontSize>
            </setFontAction>
        </action>
    </component>
    <component>
        <id>3060403</id>
        <type>Label</type>
        <attribute>
        <text>开始时间：</text>
            <rect>
                <x>400</x>
                <y>350</y>
                <width>300</width>
                <heigh>50</heigh>
            </rect>
        </attribute>
    </component>
    <component>
        <id>3060404</id>
        <type>Date</type>
        <attribute>
            <rect>
                <x>540</x>
                <y>350</y>
                <width>300</width>
                <heigh>50</heigh>
            </rect>
        </attribute>
    </component>
    <component>
        <id>3060405</id>
        <type>Label</type>
        <attribute>
        <text>结束时间：</text>
            <rect>
                <x>400</x>
                <y>450</y>
                <width>300</width>
                <heigh>50</heigh>
            </rect>
        </attribute>
    </component>
    <component>
        <id>3060406</id>
        <type>Date</type>
        <attribute>
            <rect>
                <x>540</x>
                <y>450</y>
                <width>300</width>
                <heigh>50</heigh>
            </rect>
        </attribute>
    </component>
	<component>
        <id>3060408</id>
        <type>Button</type>
        <attribute>
            <text>确认</text>
            <rect>
                <x>500</x>
                <y>600</y>
                <width>200</width>
                <heigh>70</heigh>
            </rect>			
        </attribute>
         <action>
            <sendMsgAction>
                <prsCode>query_zhifumxcx</prsCode>
                <conditions>3060203,3060404,3060406</conditions>
                <nextStep>
                    <nextPanel>30605</nextPanel>
                    <display>3060504</display>
               </nextStep>
                <exception>
                    <nextPanel>306899</nextPanel>
                    <display>306897</display>
                </exception>
                <checkRules>
                    <notEqual>
                        <more>3060406</more>
                        <less>3060404</less>
                        <errMsg>查询开始时间不能够大于结束时间</errMsg>
                    </notEqual>
                </checkRules>
            </sendMsgAction>
        </action>
    </component>  
    <component>
        <id>3060409</id>
        <type>Button</type>
        <attribute>
            <text>&lt;html&gt;&lt;center&gt;返回&lt;br&gt;主界面&lt;center&gt;&lt;/html&gt;</text>
			<!--<icon>/image/105.png</icon>-->
            <rect>
                <x>1000</x>
                <y>800</y>
                <width>150</width>
                <heigh>100</heigh>
            </rect>
        </attribute>
        <action>
            <jumpPanelAction>
                <nextPanel>30</nextPanel>
            </jumpPanelAction>
        </action>
        <action> 
            <setFontAction>
                <fontSize>24</fontSize>
            </setFontAction>
        </action>
    </component>  
</panel>
